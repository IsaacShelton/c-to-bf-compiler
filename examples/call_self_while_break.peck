
u0 main(){
    pyramid(10);
}

u0 pyramid(u8 length){
    u8 i = 0;

    while(i < length){
        while(true){
            print("*");
            i++;
            break;
        }

        if(i == 5){
            break;
        }
    }

    print("\n");

    if(length > 1){
        pyramid(length - 1);
    }
}

