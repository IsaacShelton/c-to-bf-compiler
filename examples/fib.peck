
u16 fibonacci(u16 n){
    if(n <= 1){
        return n;
    } else {
        return fibonacci(n - 1) + fibonacci(n - 2);
    }
}

u0 main(){
    print("Enter a number: ");
    u8 x = readu8();

    print("fibonacci(");
    printu8(x);
    print(") = ");
    printu16(fibonacci((u16) x));
    print("\n");
}

