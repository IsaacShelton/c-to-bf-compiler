        u8 i = 0;

        while(i < 6){
            if(c == letters[i]){
                break;
            }
            i += 1;
        }

        if(i == 6){
            print("Could not find\n");
        } else {
            print("Found at index ");
            printu8(i);
            print("\n");
        }

        j += 1;
    }
}

